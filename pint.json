{
    "preset": "laravel",
    "rules": {
        "blank_line_before_statement": {
            "statements": ["return", "try"]
        },
        "class_attributes_separation": {
            "elements": {
                "const": "none",
                "property": "none"
            }
        },
        "concat_space": {
            "spacing": "one"
        },
        "not_operator_with_successor_space": false,
        "logical_operators": true
    }
}
