{
    "name": "cybex/laravel-protector",
    "description": "Protect Databases by generating Backups and Import those on non-productive Environments.",
    "keywords": [
        "cybex",
        "laravel",
        "database",
        "backup",
        "sync",
        "encryption",
        "protector"
    ],
    "homepage": "https://github.com/cybex-gmbh/laravel-protector",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marco Szulik",
            "email": "webdevelopment@cybex-online.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/support": "^11.0",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "laravel/sail": "^1.26",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Cybex\\Protector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cybex\\Protector\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cybex\\Protector\\ProtectorServiceProvider"
            ],
            "aliases": {
                "Protector": "Cybex\\Protector\\ProtectorFacade"
            }
        }
    }
}
